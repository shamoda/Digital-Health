<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="0dp"
        android:background="@color/colorPrimary"
        >


        <RelativeLayout
            android:id="@+id/Rel1"
            android:layout_width="match_parent"

            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"

                android:layout_marginTop="13dp"
                android:layout_marginEnd="190dp"

                android:text="@string/SS_functionName"
                android:textColor="@android:color/white"
                android:textSize="18sp"
                android:textStyle="bold"
                />

        </RelativeLayout>


    </Toolbar>

    <LinearLayout
        android:id="@+id/lay2"
        android:layout_width="407dp"
        android:layout_height="121dp"
        android:layout_below="@id/toolbar"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="20dp"
        android:layout_marginRight="5dp"
        android:background="@drawable/input_design"
        android:orientation="vertical">


        <ImageView
            android:id="@+id/imageView"
            android:layout_width="wrap_content"
            android:layout_height="79dp"
            android:layout_margin="30dp"
            android:background="#FFFFFF"
            android:contentDescription="@string/todo"
            android:scaleType="fitXY"


            android:src="@drawable/logo" />


    </LinearLayout>

    <androidx.cardview.widget.CardView
        android:layout_width="wrap_content"
        android:layout_height="422dp"
        android:layout_below="@id/lay2"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="30dp"
        android:layout_marginRight="10dp"
        app:cardBackgroundColor="@color/CardColor"

        app:cardCornerRadius="15dp"
        app:cardElevation="10dp">

        <RelativeLayout
            android:id="@+id/addReport"
            android:layout_width="wrap_content"
            android:layout_height="347dp"

            android:layout_marginStart="5dp"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="5dp"
            android:layout_marginBottom="5dp"
            android:background="#00DAD4D4"
            android:backgroundTint="#9EE3EEEF"
            android:orientation="vertical">


            <TextView
                android:id="@+id/Glucoselevel"
                android:layout_width="248dp"
                android:layout_height="30dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"

                android:layout_marginStart="19dp"
                android:layout_marginTop="198dp"
                android:layout_marginEnd="170dp"
                android:text="@string/GlucoseLevel"
                android:textColor="#090909"
                android:textColorHint="#090909"
                android:textSize="10sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/des"
                android:layout_width="258dp"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"

                android:layout_marginStart="19dp"
                android:layout_marginTop="249dp"
                android:layout_marginEnd="100dp"
                android:text="@string/sugarDes"
                android:textColor="#090909"
                android:textColorHint="#090909"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/unitlevel"
                android:layout_width="wrap_content"
                android:layout_height="39dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"

                android:layout_marginStart="19dp"
                android:layout_marginTop="196dp"
                android:layout_marginEnd="21dp"
                android:text="@string/Glulevelunit"
                android:textColor="#090909"
                android:textColorHint="#090909"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/patientID3"
                android:layout_width="144dp"
                android:layout_height="36dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"

                android:layout_marginStart="19dp"
                android:layout_marginTop="19dp"
                android:layout_marginEnd="225dp"
                android:text="@string/CustomerID"
                android:textColor="#090909"
                android:textColorHint="#090909"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/type"
                android:layout_width="245dp"
                android:layout_height="34dp"
                android:layout_alignParentTop="true"

                android:layout_alignParentEnd="true"
                android:layout_marginStart="19dp"
                android:layout_marginTop="134dp"
                android:layout_marginEnd="53dp"
                android:text="@string/BloodSugar"
                android:textColor="#090909"
                android:textColorHint="#090909"
                android:textSize="18sp"
                android:textStyle="bold" />


            <TextView
                android:id="@+id/desSu"
                android:layout_width="338dp"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="22dp"
                android:layout_marginBottom="30dp"
                android:text="@string/dessu"
                android:textColor="@color/design_default_color_on_secondary"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/textPatient"
                android:layout_width="115dp"
                android:layout_height="30dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="24dp"
                android:layout_marginTop="70dp"
                android:text="Patient Name :"
                android:textColor="#232121" />

            <TextView
                android:id="@+id/inputCusIDs"
                android:layout_width="131dp"
                android:layout_height="31dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="18dp"
                android:layout_marginEnd="44dp"
                android:text="TextView"
                android:textColor="@color/design_default_color_on_secondary" />

            <TextView
                android:id="@+id/inputpatientNames"
                android:layout_width="143dp"
                android:layout_height="31dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="70dp"
                android:layout_marginEnd="33dp"
                android:text="TextView"
                android:textColor="@color/design_default_color_on_secondary" />

            <TextView
                android:id="@+id/glucoseLevelss"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginEnd="97dp"
                android:layout_marginBottom="129dp"
                android:text="TextView"
                android:textColor="@color/design_default_color_on_secondary"
                android:textStyle="bold" />


        </RelativeLayout>
    </androidx.cardview.widget.CardView>



</RelativeLayout>